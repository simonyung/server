<div class="container" style="margin-top: 35px">
        <div class="page-header page-heading">
          <div class="clearfix"></div>
        </div>
      </div>

      </td>
      <table class="table forum table-striped">
        <thead>
        <tr>
          <th class="cell-stat"></th>
          <th>
            <h3>Posts</h3>
          </th>
          <th class="cell-stat text-center hidden-xs hidden-sm">Topics</th>
          <th class="cell-stat text-center hidden-xs hidden-sm">Comment</th>
          <th class="cell-stat-2x hidden-xs hidden-sm">Last Post</th>
        </tr>
        </thead>
        <% @posts.each do |post| %>
            <tbody>
            <tr>
              <td class="text-center"><i class="fa fa-question fa-2x text-primary"></i></td>
              <td>
                <h4><a href="#"><%= link_to post.title , post_path(post)%></a><br></h4>
              </td>
              <td class="text-center hidden-xs hidden-sm"><a href="#">9 542</a></td>
              <td class="text-center hidden-xs hidden-sm"><a href="#">89 897</a></td>
              <td class="hidden-xs hidden-sm">by <%= post.user.email %><br><small><i class="fa fa-clock-o"></i><%= post.created_at %></small></td>
            </tr>
            <td>
              <% if can? :update, Post %>
                  <% if current_user && post.editable_by?(current_user) %>
                      <%= link_to "edit", edit_post_path(post) %>
                      <%= link_to "Delete", post_path(post), method: :delete %>
                  <% end %>
              <% end %>
            </td>
        <% end %>


        </tbody>
      </table>



      <br />
      <hr />
      <br />
